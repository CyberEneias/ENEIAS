infoText.TextSize = 11
infoText.TextColor3 = Color3_fromRGB(180, 180, 190)
infoText.TextXAlignment = Enum.TextXAlignment.Left
infoText.TextYAlignment = Enum.TextYAlignment.Top
infoText.TextWrapped = true

-- ===== SETTINGS PAGE CONTENT =====
local settingsContainer = Instance.new("Frame", contentArea)
settingsContainer.Size = UDim2_new(1, -10, 1, -60)
settingsContainer.Position = UDim2_new(0, 0, 0, 55)
settingsContainer.BackgroundTransparency = 1
settingsContainer.Visible = false

local settingsLayout = Instance.new("UIListLayout", settingsContainer)
settingsLayout.Padding = UDim.new(0, 8)
settingsLayout.SortOrder = Enum.SortOrder.LayoutOrder

-- Save/Load Settings
local saveLoadContainer = Instance.new("Frame", settingsContainer)
saveLoadContainer.Size = UDim2_new(1, -5, 0, 120)
saveLoadContainer.BackgroundColor3 = Color3_fromRGB(20, 20, 28)
saveLoadContainer.BorderSizePixel = 0

local saveLoadCorner = Instance.new("UICorner", saveLoadContainer)
saveLoadCorner.CornerRadius = UDim.new(0, 8)

local saveLoadTitle = Instance.new("TextLabel", saveLoadContainer)
saveLoadTitle.Size = UDim2_new(1, -20, 0, 20)
saveLoadTitle.Position = UDim2_new(0, 12, 0, 10)
saveLoadTitle.BackgroundTransparency = 1
saveLoadTitle.Text = "üíæ Settings Management"
saveLoadTitle.Font = Enum.Font.GothamBold
saveLoadTitle.TextSize = 14
saveLoadTitle.TextColor3 = Color3_white
saveLoadTitle.TextXAlignment = Enum.TextXAlignment.Left

-- Save Button
local saveBtn = Instance.new("TextButton", saveLoadContainer)
saveBtn.Size = UDim2_new(0, 120, 0, 35)
saveBtn.Position = UDim2_new(0, 12, 0, 40)
saveBtn.BackgroundColor3 = Color3_fromRGB(80, 120, 200)
saveBtn.Text = "Save Settings"
saveBtn.Font = Enum.Font.GothamBold
saveBtn.TextSize = 13
saveBtn.TextColor3 = Color3_white

local saveCorner = Instance.new("UICorner", saveBtn)
saveCorner.CornerRadius = UDim.new(0, 6)

saveBtn.MouseButton1Click:Connect(function()
	saveSettings()
	saveBtn.Text = "‚úÖ Saved!"
	task.delay(2, function()
		saveBtn.Text = "Save Settings"
	end)
end)

-- Load Button
local loadBtn = Instance.new("TextButton", saveLoadContainer)
loadBtn.Size = UDim2_new(0, 120, 0, 35)
loadBtn.Position = UDim2_new(0, 12, 0, 80)
loadBtn.BackgroundColor3 = Color3_fromRGB(120, 80, 200)
loadBtn.Text = "Load Settings"
loadBtn.Font = Enum.Font.GothamBold
loadBtn.TextSize = 13
loadBtn.TextColor3 = Color3_white

local loadCorner = Instance.new("UICorner", loadBtn)
loadCorner.CornerRadius = UDim.new(0, 6)

loadBtn.MouseButton1Click:Connect(function()
	local settings = loadSettings()
	if settings then
		-- Apply loaded settings
		state.speedValue = settings.speedValue
		state.m1ClickDelay = settings.m1ClickDelay
		
		-- Update toggles
		if toggleUpdateFunctions.godMode then
			toggleUpdateFunctions.godMode(settings.godModeEnabled == true)
		end
		if toggleUpdateFunctions.autofarm then
			toggleUpdateFunctions.autofarm(settings.autofarmEnabled == true)
		end
		if toggleUpdateFunctions.speed then
			toggleUpdateFunctions.speed(settings.speedEnabled == true)
		end
		if toggleUpdateFunctions.infiniteJump then
			toggleUpdateFunctions.infiniteJump(settings.infiniteJumpEnabled == true)
		end
		if toggleUpdateFunctions.m1Macro then
			toggleUpdateFunctions.m1Macro(settings.m1MacroEnabled == true)
		end
		
		-- Update GUI key
		local keyCode = Enum.KeyCode[settings.guiToggleKey]
		if keyCode then
			state.guiToggleKey = keyCode
		end
		
		loadBtn.Text = "‚úÖ Loaded!"
		task.delay(2, function()
			loadBtn.Text = "Load Settings"
		end)
	end
end)

-- ===== INPUT HANDLING =====
UserInputService.JumpRequest:Connect(function()
	if state.infiniteJumpEnabled then
		local _, hum, _ = getCharComponents()
		if hum then hum:ChangeState(Enum.HumanoidStateType.Jumping) end
	end
end)

UserInputService.InputBegan:Connect(function(input, processed)
	if processed then return end

	-- F1 - Toggle GUI
	if input.KeyCode == Enum.KeyCode.F1 then
		state.guiVisible = not state.guiVisible
		mainFrame.Visible = state.guiVisible
	end

	-- F2 - Toggle M1 Macro
	if input.KeyCode == Enum.KeyCode.F2 then
		state.m1MacroEnabled = not state.m1MacroEnabled
		
		if toggleUpdateFunctions.m1Macro then
			toggleUpdateFunctions.m1Macro(state.m1MacroEnabled)
		end
		
		if state.m1MacroEnabled then
			disconnectConnection("m1macro")
			state.connections.m1macro = RunService.Heartbeat:Connect(m1MacroLoop)
		else
			disconnectConnection("m1macro")
		end
	end
end)

-- ===== CHARACTER EVENTS =====
player.CharacterAdded:Connect(function(char)
	invalidateCharCache()
	task.wait(0.3)
	
	if state.speedEnabled then
		disconnectConnection("speed")
		state.connections.speed = RunService.Heartbeat:Connect(speedLoop)
	end
	if state.m1MacroEnabled then
		disconnectConnection("m1macro")
		state.connections.m1macro = RunService.Heartbeat:Connect(m1MacroLoop)
	end
	if state.godModeEnabled then
		disconnectConnection("godmode")
		state.connections.godmode = RunService.Heartbeat:Connect(applyGodMode)
	end
end)

-- ===== AUTO-LOAD SETTINGS =====
task.spawn(function()
	task.wait(1)
	local settings = loadSettings()
	if settings then
		state.speedValue = settings.speedValue
		state.m1ClickDelay = settings.m1ClickDelay
		
		-- Update GUI key
		local keyCode = Enum.KeyCode[settings.guiToggleKey]
		if keyCode then
			state.guiToggleKey = keyCode
		end
	end
end)

-- ===== PAGE SWITCHING =====
-- Show/hide pages based on selection
RunService.Heartbeat:Connect(function()
	if state.currentPage == "combat" then
		scrollFrame.Visible = true
		settingsContainer.Visible = false
	elseif state.currentPage == "settings" then
		scrollFrame.Visible = false
		settingsContainer.Visible = true
	end
end)

-- ===== CLEANUP =====
game:GetService("Players").LocalPlayer.OnTeleport:Connect(function()
	cleanAllConnections()
	saveSettings()
end)

