-- Function to create sidebar button
local function createSidebarButton(text, icon, pageName)
	local btn = Instance.new("TextButton", sidebarList)
	btn.Size = UDim2_new(1, -10, 0, 40)
	btn.BackgroundTransparency = 1
	btn.Text = "  " .. text
	btn.Font = Enum.Font.GothamBold
	btn.TextSize = 13
	btn.TextColor3 = Color3_fromRGB(180, 180, 200)
	btn.TextXAlignment = Enum.TextXAlignment.Left
	btn.AutoButtonColor = false
	
	local iconLabel = Instance.new("TextLabel", btn)
	iconLabel.Size = UDim2_new(0, 20, 0, 20)
	iconLabel.Position = UDim2_new(1, -25, 0.5, -10)
	iconLabel.BackgroundTransparency = 1
	iconLabel.Text = icon
	iconLabel.Font = Enum.Font.GothamBold
	iconLabel.TextSize = 14
	iconLabel.TextColor3 = Color3_fromRGB(200, 150, 255)
	
	btn.MouseEnter:Connect(function()
		TweenService:Create(btn, TweenInfo.new(0.2), {TextColor3 = Color3_fromRGB(230, 180, 255)}):Play()
	end)
	
	btn.MouseLeave:Connect(function()
		if state.currentPage ~= pageName then
			TweenService:Create(btn, TweenInfo.new(0.2), {TextColor3 = Color3_fromRGB(180, 180, 200)}):Play()
		end
	end)
	
	btn.MouseButton1Click:Connect(function()
		state.currentPage = pageName
		-- Update all buttons
		for _, child in ipairs(sidebarList:GetChildren()) do
			if child:IsA("TextButton") then
				local childPage = child.Name
				if childPage == pageName then
					TweenService:Create(child, TweenInfo.new(0.2), {TextColor3 = Color3_fromRGB(200, 150, 255)}):Play()
				else
					TweenService:Create(child, TweenInfo.new(0.2), {TextColor3 = Color3_fromRGB(180, 180, 200)}):Play()
				end
			end
		end
		
		-- Update content title
		if pageName == "combat" then
			contentTitle.Text = "|| COMBAT"
			subtitle.Text = "‚ö° Advanced Combat Features"
		elseif pageName == "configs" then
			contentTitle.Text = "|| CONFIGURATIONS"
			subtitle.Text = "‚öôÔ∏è Manage Your Settings"
		end
	end)
	
	btn.Name = pageName
	
	-- Set initial state
	if pageName == "combat" then
		state.currentPage = "combat"
		TweenService:Create(btn, TweenInfo.new(0.2), {TextColor3 = Color3_fromRGB(200, 150, 255)}):Play()
	end
	
	return btn
end

-- Create sidebar buttons
createSidebarButton("COMBAT", "‚öîÔ∏è", "combat")
createSidebarButton("CONFIGS", "‚öôÔ∏è", "configs")

