-- Function to create styled input field
local function createInputField(parent, text, placeholder)
	local container = Instance.new("Frame", parent)
	container.Size = UDim2_new(1, -5, 0, 70)
	container.BackgroundColor3 = Color3_fromRGB(20, 20, 28)
	container.BorderSizePixel = 0
	
	local corner = Instance.new("UICorner", container)
	corner.CornerRadius = UDim.new(0, 8)
	
	local label = Instance.new("TextLabel", container)
	label.Size = UDim2_new(1, -20, 0, 20)
	label.Position = UDim2_new(0, 12, 0, 10)
	label.BackgroundTransparency = 1
	label.Text = text
	label.Font = Enum.Font.GothamBold
	label.TextSize = 14
	label.TextColor3 = Color3_white
	label.TextXAlignment = Enum.TextXAlignment.Left
	
	local textBox = Instance.new("TextBox", container)
	textBox.Size = UDim2_new(1, -24, 0, 35)
	textBox.Position = UDim2_new(0, 12, 0, 30)
	textBox.BackgroundColor3 = Color3_fromRGB(30, 30, 40)
	textBox.TextColor3 = Color3_white
	textBox.Font = Enum.Font.Gotham
	textBox.TextSize = 13
	textBox.PlaceholderText = placeholder
	textBox.ClearTextOnFocus = false
	
	local textBoxCorner = Instance.new("UICorner", textBox)
	textBoxCorner.CornerRadius = UDim.new(0, 6)
	
	-- Hover effect
	textBox.Focused:Connect(function()
		TweenService:Create(textBox, TweenInfo.new(0.2), {BackgroundColor3 = Color3_fromRGB(40, 40, 50)}):Play()
	end)
	
	textBox.FocusLost:Connect(function()
		TweenService:Create(textBox, TweenInfo.new(0.2), {BackgroundColor3 = Color3_fromRGB(30, 30, 40)}):Play()
	end)
	
	return container, textBox
end

-- Function to create styled button
local function createConfigButton(parent, text, color)
	local btn = Instance.new("TextButton", parent)
	btn.Size = UDim2_new(1, -5, 0, 45)
	btn.BackgroundColor3 = color
	btn.Text = text
	btn.Font = Enum.Font.GothamBold
	btn.TextSize = 14
	btn.TextColor3 = Color3_white
	btn.AutoButtonColor = false
	
	local corner = Instance.new("UICorner", btn)
	corner.CornerRadius = UDim.new(0, 8)
	
	-- Hover effects
	btn.MouseEnter:Connect(function()
		TweenService:Create(btn, TweenInfo.new(0.2), {BackgroundColor3 = Color3_fromRGB(
			math.min(color.R * 255 + 20, 255)/255,
			math.min(color.G * 255 + 20, 255)/255,
			math.min(color.B * 255 + 20, 255)/255
		)}):Play()
	end)
	
	btn.MouseLeave:Connect(function()
		TweenService:Create(btn, TweenInfo.new(0.2), {BackgroundColor3 = color}):Play()
	end)
	
	btn.MouseButton1Click:Connect(function()
		TweenService:Create(btn, TweenInfo.new(0.1), {BackgroundColor3 = Color3_fromRGB(
			math.min(color.R * 255 + 30, 255)/255,
			math.min(color.G * 255 + 30, 255)/255,
			math.min(color.B * 255 + 30, 255)/255
		)}):Play()
		task.wait(0.1)
		TweenService:Create(btn, TweenInfo.new(0.1), {BackgroundColor3 = color}):Play()
	end)
	
	return btn
end

-- Function to update config list
local function updateConfigList(scrollingFrame)
	-- Clear existing config list
	if scrollingFrame:FindFirstChild("ConfigListContainer") then
		scrollingFrame:FindFirstChild("ConfigListContainer"):Destroy()
	end
	
	local configListContainer = Instance.new("Frame", scrollingFrame)
	configListContainer.Name = "ConfigListContainer"
	configListContainer.Size = UDim2_new(1, -5, 0, 150)
	configListContainer.BackgroundColor3 = Color3_fromRGB(20, 20, 28)
	configListContainer.BorderSizePixel = 0
	
	local corner = Instance.new("UICorner", configListContainer)
	corner.CornerRadius = UDim.new(0, 8)
	
	local title = Instance.new("TextLabel", configListContainer)
	title.Size = UDim2_new(1, -20, 0, 20)
	title.Position = UDim2_new(0, 12, 0, 10)
	title.BackgroundTransparency = 1
	title.Text = "üìÅ Config List"
	title.Font = Enum.Font.GothamBold
	title.TextSize = 14
	title.TextColor3 = Color3_white
	title.TextXAlignment = Enum.TextXAlignment.Left
	
	-- Get configs
	local configs = ConfigSystem.getConfigList()
	
	if #configs == 0 then
		local emptyLabel = Instance.new("TextLabel", configListContainer)
		emptyLabel.Size = UDim2_new(1, -20, 0, 100)
		emptyLabel.Position = UDim2_new(0, 12, 0, 35)
		emptyLabel.BackgroundTransparency = 1
		emptyLabel.Text = "No configurations saved yet.\nCreate one using 'Create Config'."
		emptyLabel.Font = Enum.Font.Gotham
		emptyLabel.TextSize = 12
		emptyLabel.TextColor3 = Color3_fromRGB(150, 150, 160)
		emptyLabel.TextXAlignment = Enum.TextXAlignment.Center
		emptyLabel.TextYAlignment = Enum.TextYAlignment.Center
		emptyLabel.TextWrapped = true
		
		state.selectedConfig = nil
	else
		-- Config selection frame
		local selectionFrame = Instance.new("ScrollingFrame", configListContainer)
		selectionFrame.Size = UDim2_new(1, -24, 0, 100)
		selectionFrame.Position = UDim2_new(0, 12, 0, 35)
		selectionFrame.BackgroundColor3 = Color3_fromRGB(30, 30, 40)
		selectionFrame.BorderSizePixel = 0
		selectionFrame.ScrollBarThickness = 4
		selectionFrame.ScrollBarImageColor3 = Color3_fromRGB(100, 80, 150)
		selectionFrame.CanvasSize = UDim2_new(0, 0, 0, 0)
		selectionFrame.AutomaticCanvasSize = Enum.AutomaticSize.Y
		
		local selectionLayout = Instance.new("UIListLayout", selectionFrame)
		selectionLayout.Padding = UDim.new(0, 2)
		selectionLayout.SortOrder = Enum.SortOrder.LayoutOrder
		
		for _, configName in ipairs(configs) do
			local configBtn = Instance.new("TextButton", selectionFrame)
			configBtn.Size = UDim2_new(1, -8, 0, 30)
			configBtn.BackgroundColor3 = configName == state.selectedConfig and 
				Color3_fromRGB(80, 60, 140) or Color3_fromRGB(40, 40, 50)
			configBtn.Text = configName
			configBtn.Font = Enum.Font.Gotham
			configBtn.TextSize = 12
			configBtn.TextColor3 = Color3_white
			configBtn.AutoButtonColor = false
			
			local btnCorner = Instance.new("UICorner", configBtn)
			btnCorner.CornerRadius = UDim.new(0, 4)
			
			configBtn.MouseButton1Click:Connect(function()
				state.selectedConfig = configName
				updateConfigList(scrollingFrame)
			end)
			
			-- Hover effects
			configBtn.MouseEnter:Connect(function()
				if configName ~= state.selectedConfig then
					TweenService:Create(configBtn, TweenInfo.new(0.2), {BackgroundColor3 = Color3_fromRGB(60, 60, 70)}):Play()
				end
			end)
			
			configBtn.MouseLeave:Connect(function()
				if configName ~= state.selectedConfig then
					TweenService:Create(configBtn, TweenInfo.new(0.2), {BackgroundColor3 = Color3_fromRGB(40, 40, 50)}):Play()
				end
			end)
		end
		
		-- Show selected config
		if state.selectedConfig then
			local selectedLabel = Instance.new("TextLabel", configListContainer)
			selectedLabel.Size = UDim2_new(1, -20, 0, 20)
			selectedLabel.Position = UDim2_new(0, 12, 0, 140)
			selectedLabel.BackgroundTransparency = 1
			selectedLabel.Text = "Selected: " .. state.selectedConfig
			selectedLabel.Font = Enum.Font.GothamBold
			selectedLabel.TextSize = 11
			selectedLabel.TextColor3 = Color3_fromRGB(200, 150, 255)
			selectedLabel.TextXAlignment = Enum.TextXAlignment.Left
		end
	end
	
	return configListContainer
end

-- Function to show message
local function showMessage(parent, text, color, duration)
	local message = Instance.new("TextLabel", parent)
	message.Size = UDim2_new(1, -20, 0, 35)
	message.Position = UDim2_new(0, 10, 0, 10)
	message.BackgroundColor3 = Color3_fromRGB(30, 30, 40)
	message.TextColor3 = color
	message.Font = Enum.Font.GothamBold
	message.TextSize = 13
	message.Text = text
	message.TextWrapped = true
	message.ZIndex = 10
	
	local corner = Instance.new("UICorner", message)
	corner.CornerRadius = UDim.new(0, 8)
	
	task.delay(duration or 3, function()
		if message then
			message:Destroy()
		end
	end)
end

-- Create Configurations UI Elements
-- 1) Config Name Input
local configNameContainer, configNameBox = createInputField(configsScroll, "Config Name", "Enter configuration name...")

-- 2) Config List
local configListContainer = updateConfigList(configsScroll)

-- 3) Create Config Button
local createConfigBtn = createConfigButton(configsScroll, "‚ûï Create Config", Color3_fromRGB(80, 120, 200))
createConfigBtn.MouseButton1Click:Connect(function()
	local configName = configNameBox.Text:gsub("%s+", " "):gsub("^%s*(.-)%s*$", "%1")
	
	if configName == "" then
		showMessage(configsScroll, "Please enter a configuration name!", Color3_fromRGB(255, 100, 100))
		return
	end
	
	if configName:match("[^%w%s%-_]") then
		showMessage(configsScroll, "Invalid name! Use only letters, numbers, spaces, hyphens and underscores.", Color3_fromRGB(255, 100, 100))
		return
	end
	
	local success, err = ConfigSystem.saveConfig(configName, ConfigSystem.getCurrentSettings())
	
	if success then
		showMessage(configsScroll, "‚úÖ Configuration '" .. configName .. "' saved successfully!", Color3_fromRGB(100, 255, 100))
		state.selectedConfig = configName
		updateConfigList(configsScroll)
		configNameBox.Text = ""
	else
		showMessage(configsScroll, "‚ùå Failed to save: " .. tostring(err), Color3_fromRGB(255, 100, 100))
	end
end)

-- 4) Load Config Button
local loadConfigBtn = createConfigButton(configsScroll, "üìÇ Load Config", Color3_fromRGB(120, 80, 200))
loadConfigBtn.MouseButton1Click:Connect(function()
	if not state.selectedConfig then
		showMessage(configsScroll, "Please select a configuration first!", Color3_fromRGB(255, 200, 100))
		return
	end
	
	local settings = ConfigSystem.loadConfig(state.selectedConfig)
	
	if settings then
		ConfigSystem.applySettings(settings)
		showMessage(configsScroll, "‚úÖ Configuration '" .. state.selectedConfig .. "' loaded successfully!", Color3_fromRGB(100, 255, 100))
	else
		showMessage(configsScroll, "‚ùå Failed to load configuration!", Color3_fromRGB(255, 100, 100))
	end
end)

-- 5) Overwrite Config Button
local overwriteConfigBtn = createConfigButton(configsScroll, "‚úèÔ∏è Overwrite Config", Color3_fromRGB(200, 120, 80))
overwriteConfigBtn.MouseButton1Click:Connect(function()
	if not state.selectedConfig then
		showMessage(configsScroll, "Please select a configuration to overwrite!", Color3_fromRGB(255, 200, 100))
		return
	end
	
	local success, err = ConfigSystem.saveConfig(state.selectedConfig, ConfigSystem.getCurrentSettings())
	
	if success then
		showMessage(configsScroll, "‚úÖ Configuration '" .. state.selectedConfig .. "' overwritten successfully!", Color3_fromRGB(100, 255, 100))
	else
		showMessage(configsScroll, "‚ùå Failed to overwrite: " .. tostring(err), Color3_fromRGB(255, 100, 100))
	end
end)

-- 6) Refresh List Button
local refreshListBtn = createConfigButton(configsScroll, "üîÑ Refresh List", Color3_fromRGB(80, 200, 120))
refreshListBtn.MouseButton1Click:Connect(function()
	updateConfigList(configsScroll)
	showMessage(configsScroll, "‚úÖ Configuration list refreshed!", Color3_fromRGB(100, 255, 100))
end)

-- 7) Set as Autoload Button and display
local autoloadContainer = Instance.new("Frame", configsScroll)
autoloadContainer.Size = UDim2_new(1, -5, 0, 80)
autoloadContainer.BackgroundColor3 = Color3_fromRGB(20, 20, 28)
autoloadContainer.BorderSizePixel = 0

local autoloadCorner = Instance.new("UICorner", autoloadContainer)
autoloadCorner.CornerRadius = UDim.new(0, 8)

local autoloadTitle = Instance.new("TextLabel", autoloadContainer)
autoloadTitle.Size = UDim2_new(1, -20, 0, 20)
autoloadTitle.Position = UDim2_new(0, 12, 0, 10)
autoloadTitle.BackgroundTransparency = 1
autoloadTitle.Text = "üîß AutoLoad Settings"
autoloadTitle.Font = Enum.Font.GothamBold
autoloadTitle.TextSize = 14
autoloadTitle.TextColor3 = Color3_white
autoloadTitle.TextXAlignment = Enum.TextXAlignment.Left

local currentAutoloadLabel = Instance.new("TextLabel", autoloadContainer)
currentAutoloadLabel.Name = "CurrentAutoload"
currentAutoloadLabel.Size = UDim2_new(1, -20, 0, 20)
currentAutoloadLabel.Position = UDim2_new(0, 12, 0, 30)
currentAutoloadLabel.BackgroundTransparency = 1
currentAutoloadLabel.Text = "Current autoload config: none"
currentAutoloadLabel.Font = Enum.Font.Gotham
currentAutoloadLabel.TextSize = 12
currentAutoloadLabel.TextColor3 = Color3_fromRGB(180, 180, 190)
currentAutoloadLabel.TextXAlignment = Enum.TextXAlignment.Left

local setAutoloadBtn = createConfigButton(autoloadContainer, "‚≠ê Set as Autoload", Color3_fromRGB(200, 180, 80))
setAutoloadBtn.Size = UDim2_new(0.6, 0, 0, 35)
setAutoloadBtn.Position = UDim2_new(0.4, 5, 0, 40)

setAutoloadBtn.MouseButton1Click:Connect(function()
	if not state.selectedConfig then
		showMessage(configsScroll, "Please select a configuration first!", Color3_fromRGB(255, 200, 100))
		return
	end
	
	state.autoLoadConfig = state.selectedConfig
	
	-- Save autoload preference
	if writefile then
		local success = pcall(function()
			writefile("FFRE_Autoload.txt", state.selectedConfig)
		end)
		
		if success then
			currentAutoloadLabel.Text = "Current autoload config: " .. state.selectedConfig
			showMessage(configsScroll, "‚úÖ '" .. state.selectedConfig .. "' set as autoload configuration!", Color3_fromRGB(100, 255, 100))
		else
			showMessage(configsScroll, "‚ö†Ô∏è Autoload set but not saved to file", Color3_fromRGB(255, 200, 100))
		end
	else
		currentAutoloadLabel.Text = "Current autoload config: " .. state.selectedConfig
		showMessage(configsScroll, "‚úÖ '" .. state.selectedConfig .. "' set as autoload configuration!", Color3_fromRGB(100, 255, 100))
	end
end)

-- Load autoload config on startup
task.spawn(function()
	task.wait(1)
	if readfile and isfile then
		if isfile("FFRE_Autoload.txt") then
			local success, autoloadName = pcall(function()
				return readfile("FFRE_Autoload.txt")
			end)
			
			if success and autoloadName and autoloadName ~= "" then
				state.autoLoadConfig = autoloadName
				currentAutoloadLabel.Text = "Current autoload config: " .. autoloadName
				
				-- Auto-load the configuration
				local settings = ConfigSystem.loadConfig(autoloadName)
				if settings then
					ConfigSystem.applySettings(settings)
					print("[AUTOLOAD] Loaded configuration: " .. autoloadName)
				end
			end
		end
	end
end)

-- ===== INPUT HANDLING =====
UserInputService.JumpRequest:Connect(function()
	if state.infiniteJumpEnabled then
		local _, hum, _ = getCharComponents()
		if hum then hum:ChangeState(Enum.HumanoidStateType.Jumping) end
	end
end)

UserInputService.InputBegan:Connect(function(input, processed)
	if processed then return end

	-- F1 - Toggle GUI
	if input.KeyCode == Enum.KeyCode.F1 then
		state.guiVisible = not state.guiVisible
		mainFrame.Visible = state.guiVisible
	end

	-- F2 - Toggle M1 Macro
	if input.KeyCode == Enum.KeyCode.F2 then
		state.m1MacroEnabled = not state.m1MacroEnabled
		
		if toggleUpdateFunctions.m1Macro then
			toggleUpdateFunctions.m1Macro(state.m1MacroEnabled)
		end
		
		if state.m1MacroEnabled then
			disconnectConnection("m1macro")
			state.connections.m1macro = RunService.Heartbeat:Connect(m1MacroLoop)
		else
			disconnectConnection("m1macro")
		end
	end
end)

-- ===== CHARACTER EVENTS =====
player.CharacterAdded:Connect(function(char)
	invalidateCharCache()
	task.wait(0.3)
	
	if state.speedEnabled then
		disconnectConnection("speed")
		state.connections.speed = RunService.Heartbeat:Connect(speedLoop)
	end
	if state.m1MacroEnabled then
		disconnectConnection("m1macro")
		state.connections.m1macro = RunService.Heartbeat:Connect(m1MacroLoop)
	end
	if state.godModeEnabled then
		disconnectConnection("godmode")
		state.connections.godmode = RunService.Heartbeat:Connect(applyGodMode)
	end
end)

-- ===== PAGE SWITCHING =====
RunService.Heartbeat:Connect(function()
	if state.currentPage == "combat" then
		scrollFrame.Visible = true
		configsContainer.Visible = false
	elseif state.currentPage == "configs" then
		scrollFrame.Visible = false
		configsContainer.Visible = true
	end
end)

-- ===== CLEANUP =====
game:GetService("Players").LocalPlayer.OnTeleport:Connect(function()
	cleanAllConnections()
end)

